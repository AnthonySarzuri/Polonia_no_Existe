  "host": "localhost",
  "basePath": "/proyecto/v1",
  "tags": [
    {
      "name": "players",
      "description": "Everything about the players"
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/players": {
      "get": {
        "tags": [
          "players"
        ],
        "summary": "Students list",
        "description": "Obten la lista de los jugadores",
        "operationId": "list",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "list"
          }
        }
      },
      "post": {
        "tags": [
          "player"
        ],
        "summary": "Add player to team",
        "description": "AÃ±ade Jugadores a la lista",
        "operationId": "addPlayer",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Player that needs to be added to a team",
            "required": true,
            "schema": {
              "$ref": "#/definitions/player"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Created player"
          }
        }
      }
    }
  },
  "definitions": {
    "player": {
      "type": "object",
      "required": [
        "name",
        "age"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string",
          "example": "Juan"
        },
        "lastname": {
          "type": "string",
          "example": "Perez"
        },
        "birthDate": {
          "type": "string",
          "example": "2000-05-15"
        },
        "image": {
          "type": "string",
          "example": "https://static.wikia.nocookie.net/memes-pedia/images/a/a3/FB_IMG_1596591789564.jpg/revision/latest/top-crop/width/360/height/450?cb=20200911174317&path-prefix=es"
        },
        "position": {
          "type": "string",
          "example": "base"
        },
        "height": {
          "type": "number",
          "example": 2.1
        }
      }
    }
  },
  "externalDocs": {
